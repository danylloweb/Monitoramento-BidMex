                <ul id="card-ul" class="features-hold baraja-container">
                    @each(card in cards)
                    <li class="single-feature" title="{{card.name}}">
                        <!-- Single Feature Starts -->

                        <img src="{{card.src}}" alt="" class="feature-image"/>
                        <!-- Feature Icon -->
                        <h4 class="feature-title color-scheme">{{card.name}}</h4>
                        <p class="feature-text">
                            {{card.name}}
                        </p>

                        <a id="cardDetailsButton" ng-click="changeCard($index, $event)" data-toggle="modal"
                           data-target="#cardDetails" class="fancy-button button-line btn-col small vertical">
                            Detalhes
								<span class="icon">
									<i class="fa fa-leaf"></i>
								</span>
                        </a>

                    </li>
                    @endeach
                    <!-- Single Feature Ends -->
